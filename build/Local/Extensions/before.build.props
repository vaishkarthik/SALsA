<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
    <CBTModuleRestoreCommand>$(CBTLocalPath)\NuGet\NuGet.exe</CBTModuleRestoreCommand>
    <NuGetMsBuildPath>$(MSBuildBinPath)</NuGetMsBuildPath>
    <CBTModuleRestoreCommandAdditionalArguments>$(CBTModuleRestoreCommandAdditionalArguments) -MSBuildPath &quot;$(NuGetMsBuildPath)&quot;</CBTModuleRestoreCommandAdditionalArguments>
  </PropertyGroup>

  <PropertyGroup>
    <Configuration Condition=" '$(Configuration)'=='' ">debug</Configuration>
    <Platform Condition=" '$(Platform)'=='' ">x64</Platform>
  </PropertyGroup>
</Project>