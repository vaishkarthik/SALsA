<Project ToolsVersion="15.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <Import Project="$(MSBuildExtensionsPath)\$(MSBuildToolsVersion)\Microsoft.Common.props" />

  <PropertyGroup>
    <OutputType>library</OutputType>
    <AssemblyName>Basic.ClassLibrary</AssemblyName>
    <ProjectGuid>{0852A7CA-7A48-4A46-9C84-6CB063257272}</ProjectGuid>
    <OutputPath>bin\$(Configuration)\</OutputPath>
    <BuildVersionXml>$(MSBuildThisFileDirectory)versions.xml</BuildVersionXml>
  </PropertyGroup>

  <Import Project="$(EnlistmentRoot)\Build\signing\StrongName.props" Condition="Exists('$(EnlistmentRoot)\Build\signing\StrongName.props')" />

  <ItemGroup>
    <NuspecFiles Include="$(MSBuildProjectDirectory)\NugetIncludeFile.txt">
      <NuspecFilesTarget>lib/net40</NuspecFilesTarget>
    </NuspecFiles>
  </ItemGroup>

  <ItemGroup>
    <Compile Include="Simple.cs" />
  </ItemGroup>

  <Import Project="$(MSBuildToolsPath)\Microsoft.CSharp.targets" />
  
  <ItemGroup>
    <QCustomInput Include="@(NuspecFiles)">
      <Visible>false</Visible>
    </QCustomInput>
    <QCustomInput Include="$(MSBuildThisFileDirectory)versions.xml">
      <Visible>false</Visible>
    </QCustomInput>
  </ItemGroup>
</Project>
